{
  "name": "@resumer/shared-types",
  "version": "0.0.0",
  "description": "Shared TypeScript types for Resumer monorepo",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "typescript": "^5.7.2",
    "rimraf": "^6.0.1"
  },
  "peerDependencies": {
    "mongoose": "^9.0.0"
  },
  "peerDependenciesMeta": {
    "mongoose": {
      "optional": true
    }
  }
}
